{
  "name": "@mathdoy/toggle-middleware",
  "version": "1.0.11",
  "description": "Toggle feature, a feature flag library for your middleware backend",
  "keywords": [
    "Toggle",
    "features",
    "feature flags",
    "flags",
    "feature toggle",
    "feature switch",
    "middleware",
    "restify",
    "fastify",
    "express"
  ],
  "main": "lib/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "authors": [
    "Mathieu Doyon"
  ],
  "repository": "MathieuDoyon/toggle",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./lib && cross-env NODE_ENV=production babel ./src -d ./lib --ignore spec.js,test.js",
    "watch": "rimraf ./lib && cross-env NODE_ENV=production babel --watch ./src -d ./lib --ignore spec.js,test.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.2.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0"
  }
}
